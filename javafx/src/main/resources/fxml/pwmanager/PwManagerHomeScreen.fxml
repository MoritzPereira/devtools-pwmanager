<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="controlAnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hhn.it.devtools.javafx.controllers.pwmanager.PwManagerHomeScreenController">
    <children>
        <VBox layoutX="-1.0" prefHeight="602.0" prefWidth="947.0">
            <children>
                <Button fx:id="buttonChangeMasterpassword" mnemonicParsing="false" onMouseClicked="#buttonClicked" text="change Masterpassword">
                    <VBox.margin>
                        <Insets left="765.0" top="40.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="buttonAddEntry" mnemonicParsing="false" onMouseClicked="#buttonClicked" text="add Entry">
                    <VBox.margin>
                        <Insets left="40.0" top="20.0" />
                    </VBox.margin>
                </Button>
            <Button fx:id="buttonEdit" mnemonicParsing="false" onMouseClicked="#buttonClicked" text="edit">
               <VBox.margin>
                  <Insets left="871.0" />
               </VBox.margin>
            </Button>
                <TableView fx:id="tableView" prefWidth="200.0">
                    <VBox.margin>
                        <Insets left="30.0" right="40.0" top="20.0" />
                    </VBox.margin>
                </TableView>
            <Button fx:id="buttonPassword" mnemonicParsing="false" onMouseClicked="#buttonClicked" text="passwort" />
            <Button fx:id="buttonLogout" mnemonicParsing="false" onMouseClicked="#buttonClicked" text="logout">
               <VBox.margin>
                  <Insets bottom="10.0" left="857.0" />
               </VBox.margin></Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
